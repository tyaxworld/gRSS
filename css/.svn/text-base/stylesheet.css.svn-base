root {
    display: block;
}
body {
background:url("../images/header-background.gif") repeat scroll 0 0 transparent;
margin:0;
padding:0;
font-family:"Lucida Grande","Lucida Sans Unicode",Verdana,Arial,Helvetica,sans-serif;
font-size:12px;
}
img {
border:0 none;
}
a:link, a:visited {
text-decoration: none;
}
ul{
    margin: 0;
    padding: 0;
}
div#container {
margin: auto;
margin-top:0;
text-align:left;
width:980px;
}
/*
header
*/
div#header {
    clear: both;
    padding: 0;
    margin: 0;
    position: relative;
    width: auto;
    z-index: 10;
}
div#headerbar{
/*    background: url('../images/headerbar_bg.png') repeat-x scroll 0 100% #000000;*/
    clear: both;
    color: #969696;
    height: 95px;
    overflow: hidden;
    width: auto;
}
div#logo{

    background: #FFFFFF;
    -moz-border-radius:10px 10px 10px 10px;
    border:1px solid #CCCCCC;
    float: left;
    width: 450px;
    padding: 20px;
    margin: 10px;
}

#loginForm {
    display: none;
    height: 130px;
    width: 407px;
}

div#login_menu{
    float: right;
    padding: 10px;
}
h2.bullet-title {
    color: #0257D1;
    font-family: 'Arial',serif;
    font-size: 15px;
    text-align: center;
}
#forgotten-button {
    color: #646464;
    float: right;
    font-family: 'Tahoma',serif;
    font-size: 11px;
    margin-right: 11px;
    margin-top: 6px;
    text-decoration: none;
    text-shadow: 0 1px #FFFFFF;
}
#register-button {
    color: #0257D1;
    float: right;
    font-family: 'Tahoma',serif;
    font-size: 11px;
    margin-right: 11px;
    margin-top: 6px;
    text-decoration: none;
    text-shadow: 0 1px #FFFFFF;
}
#closeLogin, #closeProfile {
    background: url("../images/sprites.gif") no-repeat scroll -498px -259px transparent;
    float: right;
    height: 19px;
    margin-right: 11px;
    outline: medium none;
    overflow: hidden;
    text-indent: -2000px;
    width: 18px;
}
#closeLogin:hover, #closeProfile:hover {
		background-position: -516px -259px;
                cursor: pointer;
	}
#reguser-label, #loguser-label {
    background-position: -194px -453px;
}
label.loglabel {
    background: url("../images/sprites.gif") no-repeat scroll 0 0 transparent;
    display: inline;
    float: left;
    height: 23px;
    margin: 0 10px 4px 0;
    padding: 5px 0 0 27px;
    width: 166px;
}
label.loglabel input, #qu-parent input {
    border: 0 none;
    color: #999999;
    font-family: 'Tahoma',serif;
    font-size: 11px;
    width: 160px;
}
#logpass-label {
    margin-right: 0;
    width: 132px;
}
#regpass-label, #regpass2-label, #logpass-label, #editpass-label, #passold-label, #pass1-label, #pass2-label {
    background-position: 0 -453px;
}
#logpass-label input {
    width: 126px;
}
label.loglabel input, #qu-parent input {
    border: 0 none;
    color: #999999;
    font-family: 'Tahoma',serif;
    font-size: 11px;
    width: 160px;
}
fieldset {
    border: 0 none;
    margin: 0;
    outline: 0 none;
    padding: 0;
}
.red {
	color: #f00;
        font-size: 18px;
}
#sign_up span.error{
	color: #f00;
}
#login-submit-button:hover { background-position: -423px -453px; }

#login-submit-button {
    background: url("../images/sprites.gif") no-repeat scroll -388px -453px transparent;
    color: #FFFFFF;
    display: block;
    float: left;
    height: 28px;
    overflow: hidden;
    text-decoration: none;
    text-indent: -2000px;
    cursor: pointer;
    border: none;
    width: 34px;
}
h2 {
    color: #065AD2;
    font-family: 'Tahoma','Arial',serif;
    font-size: 15px;
    margin: 0 0 10px;
    text-shadow: 0 1px #FFFFFF;
}
#buttons {
    background: #FFFFFF;
    -moz-border-radius:10px 10px 10px 10px;
    border:1px solid #CCCCCC;
    font-family: 'Tahoma',serif;
    font-size: 11px;
    height: 22px;
    margin: 8px 0 0;
    padding: 10px 0 0;
/*    width: 395px;*/
}

#button1 {
    display: block;
    float: left;
    height: 16px;
    margin: 0 20px;
    text-align: center;
    text-decoration: none;
    width: 102px;
}
#button2, #button3, #button4 {
    display: block;
    float: left;
    height: 16px;
/*    margin: 0 0 0 51px;*/
    text-align: center;
    text-decoration: none;
    width: 152px;
}
div#menubar {
    clear: both;
    margin: 0;
    padding: 0 10px 0 40px;
    position: relative;
    width: auto;
}
div#menubar div.menubar-l {
    background:url("../images/menubar_l.png") no-repeat scroll 0 0 transparent;
    height:40px;
    left:0;
    position:absolute;
    top:0;
    width:40px;
}
div#menubar div.menubar-r {
    background:url("../images/menubar_r.png") no-repeat scroll 0 0 transparent;
    height:40px;
    position:absolute;
    right:0;
    top:0;
    width:10px;
}
div#menubar div.menubar-m {
    background:url("../images/menubar_m.png") repeat-x scroll 0 0 transparent;
    height:40px;
}
div#menu {
    background:url("../images/menu_sep_black.png") no-repeat scroll 0 0 transparent;
    height:40px;
    left:40px;
    padding:0 0 0 1px;
    position:absolute;
    top:95px;
    z-index:11;
}
div#menu ul {
    float:left;
    list-style:none outside none;
    margin:0;
    padding:0;
}
div#menu ul.menu {
    background:url("../images/menu_sep_white.png") no-repeat scroll 100% 0 transparent;
    padding-right:1px;
}
div#menu li {
    display:block;
    float:left;
    margin:0;
    padding:0;
    position:relative;
    z-index:5;
}
div#menu li.level1 {
    background:url("../images/menu_sep_white.png") no-repeat scroll 0 0 transparent;
}
div#menu li a, #menu li span.separator {
    color:#646464;
    display:block;
    float:left;
    height:40px;
    line-height:32px;
    text-decoration:none;
}
div#menu a.level1, div#menu span.level1 {
    background:url("../images/menu_sep_black.png") no-repeat scroll 100% 0 transparent;
    color:#464646;
    font-size:135%;
    line-height:250%;
}
div#menu a.level1 span, div#menu span.level1 span {
    padding:0 20px;
}
/*
header end
*/
/*
sign up form
*/
#sign_up {
    -moz-border-radius: 6px 6px 6px 6px;
    -moz-box-shadow: 0 2px 12px rgba(0, 0, 0, 0.6);
    background: none repeat scroll 0 0 #EEF2F7;
    border: 1px solid #536376;
    display: none;
    padding: 14px 22px;
    position: relative;
    width: 400px;
}
/******* FORM *******/
h1 {
    border-bottom: 1px dotted #6B9EF1;
    color: #5F95EF;
    font-size: 18px;
    font-weight: 700;
    line-height: 1.2em;
    margin-bottom: 1em;
}
#customForm{
	padding: 0 5px 5px;
}
#customForm label{
	display: block;
	color: #797979;
	font-weight: 700;
	line-height: 1.4em;
}
#customForm input{
	width: 220px;
	padding: 6px;
	color: #949494;
	font-family: Arial,  Verdana, Helvetica, sans-serif;
	font-size: 11px;
	border: 1px solid #cecece;
}
#customForm input.error{
	background: #f8dbdb;
	border-color: #e77776;
}
#customForm textarea{
	width: 550px;
	height: 80px;
	padding: 6px;
	color: #adaeae;
	font-family: Arial,  Verdana, Helvetica, sans-serif;
	font-style: italic;
	font-size: 12px;
	border: 1px solid #cecece;
}
#customForm textarea.error{
	background: #f8dbdb;
	border-color: #e77776;
}
#customForm div{
	margin-bottom: 15px;
}

#customForm div span{
/*	color: #b1b1b1;*/
        color: #0257D1;
	font-size: 11px;
}

#customForm div span.error{
	color: #e46c6e;
}
#customForm #send{
	background: #6f9ff1;
	color: #fff;
	font-weight: 700;
	font-style: normal;
	border: 0;
	cursor: pointer;
}
#customForm #send:hover{
	background: #79a7f1;
}
#error{
	margin-bottom: 20px;
	border: 1px solid #efefef;
}
#error ul{
	list-style: square;
	padding: 5px;
	font-size: 11px;
}
#error ul li{
	list-style-position: inside;
	line-height: 1.6em;
}
#error ul li strong{
	color: #e46c6d;
}
#error.valid ul li strong{
	color: #93d72e;
}
