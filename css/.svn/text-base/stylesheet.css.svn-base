#container a:link, a:visited {
color: #3C78A7;
text-decoration: none;
}
div #up {
margin-top: 5px;
text-align: left;
width: 980px;
}
.round {
	background: url("../images/leftside.gif") repeat-y left top #FFFFFF;
	margin: 5px 0 0;
}
.top {
	background: url('../images/top.gif') no-repeat scroll left top transparent;
	height: 20px;
	width: 100%;
}
.top span {
background: url("../images/top-right.gif") no-repeat scroll right top transparent;
display: block;
height: 20px;
position:relative;
}
#up .content {
background: url('../images/rightside.gif') repeat-y scroll right top transparent;
padding: 1px 15px 0;
min-height: 100px;
}
.label {
    height: 15px;
}
.bottom-white{
background: url('../images/bottom_upper.gif') no-repeat scroll left bottom transparent;
}
.bottom-white span {
background:url("../images/bottom-upper-right.gif") no-repeat scroll right top transparent;
display:block;
height:20px;
position:relative;
}
div#most-popular{
float: right;
width: 326px;
}
.bottom{
background:url("../images/bottom.gif") no-repeat scroll left bottom transparent;
height:60px;
width:100%;
}
.bottom span {
background:url("../images/bottom-right.gif") no-repeat scroll right top transparent;
display:block;
float:right;
height:60px;
position:relative;
width:25px;
z-index:50;
}
.center-content {
background:url("../images/rightside.gif") repeat-y scroll right top transparent;
margin:-1px 0 -50px;
padding:10px 10px;
position:relative;
z-index:40;
}

.center-content a img{
border: 1px solid #CCCCCC;
margin-right: 5px;
margin-top: 5px;
margin-bottom: 5px;
padding: 2px;
}
.left, .alignleft {
float:left;
}
.head {
font: 17px Georgia, Arial, Helvetica,sans-serif;
text-align: center;
}
div#most-popular li {
border-bottom: 1px dotted #CCCCCC;
clear: left;
list-style: none outside none;
}
.slider-buttons{
padding: 10px 0 0;
text-align: center;
}
div#services ul {
height:150px;
margin:0;
padding:0;
}
div#services ul li {
-moz-border-radius:10px 10px 10px 10px;
border:1px solid #CCCCCC;
float:left;
list-style:none outside none;
margin:8px 6px;
padding:5px;
width:210px;
}
div#services ul li a {
color:#000000;
font-size:13px;
font-weight:bold;
padding:16px 0 16px 50px;
}
#headline {
    width:950px;
    height: 100px;
    overflow: hidden;
    background:#fff;
    margin:0px 0px 0px 0px;
    font-size:1.05em;
    line-height:1.5em;
    position:relative;
}
div.article {
background:none repeat scroll 0 0 #FFFFFF;
width:100%;
}
div.image {
float:left;
padding-top:5px;
}
div#news-sl{
    float: left;
    width: 620px;
}
div.spacer {
clear:both;
font-size:0;
height:0;
line-height:0;
}
.glidecontentwrapper {
height:250px;
overflow:hidden;
position:relative;
width:auto;
}
.glidecontenttoggler {
margin-right: 10px;
margin-bottom:5px;
background: url('../images/sites_logos/dir_logo.gif') no-repeat scroll right top transparent ;
height: 33px;
text-align:center;
width:auto;
}

.glidecontent {
background:none repeat scroll 0 0 #F7F7F7;
height:100%;
position:absolute;
visibility:hidden;
width:969px;
}
.glidecontent ul {
margin:0;
padding:0;
}
.glidecontent ul li {
display:block;
float:left;
margin:0 35px;
padding:0;
width:170px;
}
.glidecontent ul li .head {
padding:6px 0;
font-size:15px;
font-weight:bold;
}
.glidecontent ul li .head a {
	color:#666666;

}
.glidecontent ul li img {
	margin:3px 0;
}
.glidecontent .topics {
	height:72px;
	overflow:hidden;
	width:150px;
}
.topics a {
	font-size:12px;
	line-height:17px;
}
a.headline {
	font:bold 12px Verdana,Arial,Helvetica,sans-serif;
}
#hot-topics .content {
	background:url("../images/rightside.gif") repeat-y scroll right top transparent;
	padding:0 5px;
}
.glidecontenttoggler a {
	-moz-border-radius:3px 3px 3px 3px;
	background:url("../images/pagination_bgr.gif") repeat-x scroll left bottom transparent;
	border:1px solid #E3E3E3;
	display:inline-block;
	margin-right:5px;
	padding:3px 6px;
	text-decoration:none;
}
.glidecontenttoggler a:hover {
	background:url("../images/pagination_bgrON.gif") repeat-x scroll left bottom transparent;
	border:1px solid #CC0000;
}
.glidecontenttoggler a.selected {
	background:url("../images/pagination_bgrON.gif") repeat-x scroll left bottom transparent;
	border:1px solid #CC0000;
}
.glidecontenttoggler a.prev:hover, .glidecontenttoggler a.next:hover {
	background:url("../images/pagination_bgrON.gif") repeat-x scroll left bottom transparent;
	border:1px solid #CC0000;
}
